{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "IdentityDb": "Data Source=App_Data/licensewatch.identity.db",
    "AppDb": "Data Source=/app/App_Data/licensewatch.app.db"
  },
  "Security": {
    "SeedAdmin": {
      "Enabled": false
    }
  },
  "Attachments": {
    "MaxSizeMb": 10,
    "AllowedExtensions": [ ".pdf", ".png", ".jpg", ".jpeg", ".docx", ".xlsx", ".txt" ]
  }
}
